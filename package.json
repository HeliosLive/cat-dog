{
  "name": "cat-dog",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:changed": "ng test --only-changed",
    "test:watch": "jest --watch",
    "jest:clear": "jest --clearCache",
    "test:ci": "jest --coverage --reporters=jest-junit --reporters=default",
    "postinstall": "ngcc",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.3.0",
    "@angular/common": "13.3.0",
    "@angular/compiler": "13.3.0",
    "@angular/core": "13.3.0",
    "@angular/forms": "13.3.0",
    "@angular/platform-browser": "13.3.0",
    "@angular/platform-browser-dynamic": "13.3.0",
    "@angular/router": "13.3.0",
    "angular-svg-icon": "13.0.0",
    "lottie-web": "5.9.4",
    "ngx-lottie": "8.2.0",
    "rxjs": "7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.0",
    "@angular/cli": "13.3.0",
    "@angular/compiler-cli": "13.3.0",
    "@ngneat/spectator": "10.0.1",
    "@types/jest": "27.5.0",
    "@types/node": "^12.11.1",
    "jest": "27.5.1",
    "jest-junit": "13.2.0",
    "jest-location-mock": "1.0.9",
    "jest-preset-angular": "11.1.2",
    "jest-sonar-reporter": "2.0.0",
    "ng-mocks": "13.5.1",
    "rxjs-marbles": "7.0.1",
    "typescript": "~4.6.2"
  },
  "jest-junit": {
    "outputDirectory": "./junit"
  },
  "jestSonar": {
    "reportPath": "coverage/sonarqube-report",
    "reportFile": "sonarqube-report.xml",
    "indent": 4
  }
}
